{
	"name": "@jellydn/eslint-fixer",
	"version": "0.3.1",
	"keywords": ["fixer", "typescript", "eslint", "eslint-fixer"],
	"license": "MIT",
	"author": "Huynh Duc Dung",
	"type": "module",
	"bin": "./dist/cli.mjs",
	"files": ["dist"],
	"scripts": {
		"build": "pkgroll --minify --sourcemap --clean-dist --env.NODE_ENV=production",
		"check": "biome check --write",
		"cli": "npx tsx ./src/cli.ts",
		"compile": "bun build ./index.ts --outfile ./bin/eslint-fixer --compile --target bun-darwin-arm64",
		"dev": "bun run index.ts",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"@eslint/js": "9.13.0",
		"@types/eslint__js": "8.42.3",
		"cleye": "1.3.2",
		"eslint": "9.13.0",
		"typescript-eslint": "7.18.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/bun": "latest",
		"pkgroll": "2.5.0",
		"tsx": "4.19.1"
	},
	"peerDependencies": {
		"typescript": "^5.5.3"
	}
}
