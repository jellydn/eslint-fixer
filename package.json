{
	"name": "@productsway/eslint-fixer",
	"version": "0.1.0",
	"keywords": ["fixer", "typescript"],
	"license": "MIT",
	"author": "Huynh Duc Dung",
	"type": "module",
	"module": "index.ts",
	"bin": "./dist/index.js",
	"files": ["dist"],
	"scripts": {
		"build": "bun build index.ts  --outdir ./dist --target node",
		"check": "biome check --write",
		"compile": "bun build ./index.ts --outfile ./bin/eslint-fixer --compile --target bun-darwin-arm64",
		"dev": "bun run index.ts",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"@eslint/js": "9.6.0",
		"@types/eslint__js": "8.42.3",
		"eslint": "9.6.0",
		"typescript-eslint": "7.16.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "^5.5.3"
	}
}
